{
  "version": 2,
  "builds": [
    {
      "src": "WebGl/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/WebGl/$1"
    }
  ],
  "buildCommand": "git lfs install && git lfs pull",
  "headers": [
    {
      "source": "/WebGl/Build/(.*)\\.wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/WebGl/Build/(.*)\\.data",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/WebGl/Build/(.*)\\.framework\\.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    }
  ]
}
